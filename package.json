{
    "name": "typescript-override",
    "displayName": "TypeScript-Override",
    "description": "TypeScript Override Snippets",
    "version": "0.0.1",
    "publisher": "errnull",
    "license": "GPL",
    "licenseUrl": "LICENSE",
    "engines": {
        "vscode": "^1.16.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yyued/TypeScript-Override.git"
    },
    "bugs": {
        "url": "https://github.com/yyued/TypeScript-Override/issues",
        "email": "errnull@163.com"
    },
    "categories": [
        "Snippets", "Other"
    ],
    "activationEvents": [
        "onLanguage:typescript",
        "onLanguage:typescriptreact"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.tsoverride",
                "title": "TypeScript Override Snippets"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "TypeScript Override 配置",
            "properties": {
            }
        }
    },
    "capabilities" : {
        "completionProvider" : {
            "resolveProvider": "true"
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}